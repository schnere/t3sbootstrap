{
  "name": "t3sbs/t3sbootstrap",
  "description": "Startup extension to use bootstrap 4 classes, components and more out of the box. Example and info: www.t3sbootstrap.de",
  "keywords": ["typo3", "typo3 cms", "bootstrap"],
  "homepage": "https://t3sbootstrap.de",
  "authors": [
    {
      "name": "Helmut Hackbarth",
      "email": "info@t3solution.de"
    }
  ],
  "license": "GPL-2.0+",
  "type": "typo3-cms-extension",
  "require": {
    "typo3/cms-core": ">=10.2.2, <10.9.99",
    "gridelementsteam/gridelements": ">=10.0.0, <10.9.99",
    "svewap/ws-scss": ">=1.1.13"
  },
  "autoload": {
    "psr-4": {
      "T3SBS\\T3sbootstrap\\": "Classes/"
    }
  },
  "replace": {
    "typo3-ter/t3sbootstrap": "self.version"
  }
}
